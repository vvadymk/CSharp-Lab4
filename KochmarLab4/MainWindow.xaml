        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ContentControl Name="_contentControl" Margin="10" IsEnabled="{Binding IsControlEnabled, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}"></ContentControl>
    </Grid>
